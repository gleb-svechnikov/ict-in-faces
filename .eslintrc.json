{
    "parser":  "vue-eslint-parser",
    "parserOptions": {
        "parser": {
          "js": "@babel/eslint-parser",
          "ts": "@typescript-eslint/parser"
        },
        "ecmaVersion": "latest",
        "requireConfigFile": false
    },
    "plugins": [
        "vue",
        "editorconfig",
        "vue-scoped-css"
    ],
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-recommended"
    ],
    "env": {
        "browser": true,
        "jquery": true
    },
    "ignorePatterns": ["test.ts"],
    "rules": {
        "no-new": "off",
        "no-param-reassign": "off",
        "no-console": "off"
    }
}


