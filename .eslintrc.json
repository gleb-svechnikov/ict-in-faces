{
    "parser":  "vue-eslint-parser",
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": "latest",
        "requireConfigFile": false
    },
    "plugins": [
        "vue",
        "editorconfig"
    ],
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-recommended"
    ],
    "env": {
        "browser": true,
        "jquery": true
    },
    "ignorePatterns": ["test.ts"],
    "rules": {
        "no-param-reassign": "off",
        "no-else-return": "off",
        "no-lonely-if": "off",
        "vue/return-in-computed-property": "off",
        "consistent-return": "off",
        "no-console": "off",
        "linebreak-style": "off"
    }
}


