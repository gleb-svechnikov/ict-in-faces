{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "requireConfigFile": false
    },
    "extends": ["airbnb-base"],
    "env": {
        "browser": true
    },
    "ignorePatterns": ["vue.js"],
    "rules": {
        "no-param-reassign": "off"
    }
}
