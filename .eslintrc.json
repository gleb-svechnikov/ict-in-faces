{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "requireConfigFile": false
    },

    "extends": ["airbnb-base"],
    "globals": {
        "document": "writable"
    },
    "rules": {

    "linebreak-style": "off",
    "quotes": "off",
    "no-use-before-define": "off",
    "prefer-template": "off",
    "import/order": "off",
    "import/extensions": "off"
    }
}
