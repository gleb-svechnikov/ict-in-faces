{
    "parser":  "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "requireConfigFile": false
    },
    "plugins": [
        "vue"
    ],
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-recommended"
    ],
    "env": {
        "browser": true
    },
    "ignorePatterns": ["header.js"],
    "rules": {
        "no-param-reassign": "off"
    }
}


